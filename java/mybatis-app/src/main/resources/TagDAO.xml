<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xiongxin.app.dao.TagDAO">


    <insert id="insert" parameterType="tag" useGeneratedKeys="true">
        insert into tag (name, pid,fruit, example) values (#{name}, #{pid}, #{fruit}, #{example})
    </insert>

    <select id="selectById" parameterType="integer" resultType="tag">
        select * from tag where id = #{id}
    </select>

    <update id="updateById" parameterType="integer">
        update tag set pid = 1 where id = #{id}
    </update>
</mapper>